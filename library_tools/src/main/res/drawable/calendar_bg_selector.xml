<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">

    <item app:tsquare_state_range_middle="true">
        <color android:color="@color/theme_color_bule" />
    </item>
    <item android:state_selected="true">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@color/theme_color_bule" />

            <size android:width="@dimen/dp_25" android:height="@dimen/dp_25" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@color/theme_color_bule" />

            <size android:width="@dimen/dp_25" android:height="@dimen/dp_25" />
        </shape>
    </item>
    <item app:tsquare_state_current_month="false">
        <color android:color="@color/calendar_inactive_month_bg" />
    </item>
    <item app:tsquare_state_highlighted="true">
        <color android:color="@color/calendar_highlighted_day_bg" />
    </item>
    <item app:tsquare_state_today="true">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@color/calendar_state_today_bg" />

            <size android:width="@dimen/dp_25" android:height="@dimen/dp_25" />
        </shape>
    </item>
    <item app:tsquare_state_unselect="true">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@color/calendar_state_unselect_bg" />

            <size android:width="@dimen/dp_25" android:height="@dimen/dp_25" />
        </shape>
    </item>
    <item>
        <color android:color="@color/calendar_active_month_bg" />
    </item>

</selector>