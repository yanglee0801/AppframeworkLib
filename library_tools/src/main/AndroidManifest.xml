                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ui.activity.ApplyForLeave" />
        <activity android:name=".ui.activity.LeaveActivity" />
        <activity android:name=".ui.activity.TextActivity">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>
        <activity android:name=".ui.activity.WebViewUtilActivity" />
        <activity android:name=".timessquare.SampleTimesSquareActivity" />
    </application>

</manifest>